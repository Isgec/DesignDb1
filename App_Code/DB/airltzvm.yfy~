            Sql &= "            when 007 then 'Open KM (DMS)' "
            Sql &= "            End) As t_issu,t_retn,t_isby,t_isdt from tdmisg131200 "
            Sql &= "LEFT JOIN tdmisg130200 "
            Sql &= "on tdmisg131200.t_user= tdmisg130200.t_logn "
            Sql &= "where (t_tran LIKE ('SMD%') OR t_tran LIKE ('BOI%') OR t_tran LIKE ('APC%') OR t_tran LIKE ('EPC%') OR t_tran LIKE ('PCD%')) "
            Sql &= " and t_dprj = ('" & PrjID & "') and tdmisg130200.t_dept ='PRC' and tdmisg131200.t_stat =8"

          Case "Process_Transmittal_Returned"

            Sql = "Select t_tran,(Case t_type   "
            Sql &= "            when 1 then 'Customer' "
            Sql &= "            when 2 then 'Internal' "
            Sql &= "            When 3 Then 'Site' "
            Sql &= "            when 4 then 'Vendor' "
            Sql &= "            End) As t_type,t_subj,(Case t_stat "
            Sql &= "            when 1 then 'Returned' "
            Sql &= "            When 2 Then 'Free' "
            Sql &= "            when 3 then 'Under Approval' "
            Sql &= "            When 4 Then 'Under Issue' "
            Sql &= "            when 5 then 'Issued' "
            Sql &= "            When 6 Then 'Patial Released' "
            Sql &= "            when 7 then 'Received' "
            Sql &= "            when 8 then 'Closed' "
            Sql &= "            End) As t_stat,t_user,(case tdmisg130200.t_dept"
            Sql &= " when 'PRC' then 'PROCESS' "
            Sql &= "when 'ELE' then 'ELECTRICAL' "
            Sql &= "when 'MEC' then 'MECHANICAL' "
            Sql &= "when 'OTHERS' then 'OTHERS' "
            Sql &= "WHEN 'STR' THEN 'STRUCTURE' "
            Sql &= "WHEN 'PIP' THEN 'PIPING' "
            Sql &= "WHEN 'C&I' THEN 'C&I' "
            Sql &= " else 'OTHERS' "
            Sql &= "end) AS Owner_Dept,t_apsu,(Case t_issu "
            Sql &= "            when 001 then  'Hard copy by Hand' "
            Sql &= "            When 002 Then 'Hard copy by Courier' "
            Sql &= "            when 003 then 'e mail' "
            Sql &= "            When 004 Then 'Uploaded in ftp server' "
            Sql &= "            when 005 then 'Uploaded in client or consultant portal' "
            Sql &= "            When 006 Then 'Vault' "
            Sql &= "            when 007 then 'Open KM (DMS)' "
            Sql &= "            End) As t_issu,t_retn,t_isby,t_isdt from tdmisg131200 "
            Sql &= "LEFT JOIN tdmisg130200 "
            Sql &= "on tdmisg131200.t_user= tdmisg130200.t_logn "
            Sql &= "where (t_tran LIKE ('SMD%') OR t_tran LIKE ('BOI%') OR t_tran LIKE ('APC%') OR t_tran LIKE ('EPC%') OR t_tran LIKE ('PCD%')) "
            Sql &= " and t_dprj = ('" & PrjID & "') and tdmisg130200.t_dept ='PRC' and tdmisg131200.t_stat =1"


