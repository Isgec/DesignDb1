            Sql &= "            WHEN 'SERVICE' THEN 'OTHERS' "
            Sql &= "            else 'OTHERS' "
            Sql &= "            end) as Owner_Dept,"
            Sql &= "            (case t_sent_1 "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IMechanical, "
            Sql &= "            (case t_sent_2  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IStructure, "
