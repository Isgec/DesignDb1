            Sql &= " when 3 then 'Approved' "
            Sql &= " end) as t_stat, (case t_reqs "
            Sql &= " when 001 then 'Work Stopped' "
            Sql &= " when 002 then 'Work can proceed' "
            Sql &= " when 003 then 'Work will stop' "
            Sql &= " end) as t_reqs,(case t_comp "
            Sql &= " when 001 then 'Work Stopped' "
            Sql &= " when 002 then 'Manufacturing Yet to start' "
            Sql &= " when 003 then 'Under manufacturing' "
            Sql &= " when 004    then 'Ready for dispatch' "
            Sql &= " when 005 then 'Material Recieved at site' "
            Sql &= " when 006 then 'Erection completed' "
            Sql &= " when 007 then 'Boiler commisioned and in operation' "
            Sql &= " when 008 then 'In design stage' "
            Sql &= " when 009 then 'Budget available in element 50060100' "
            Sql &= " when 010 then 'under progress' "
            Sql &= " when 011 then 'not applicable' "
            Sql &= " when 012 then 'Budget avilable in element' "
            Sql &= " end) as t_comp,t_crea,t_user,"
            Sql &= " (case ttppdm090200.t_sort "
            Sql &= "            when 'MECH-SUGAR' then 'MECHANICAL' "
            Sql &= "            when 'ELECTRICAL' then 'ELECTRICAL' "
            Sql &= "            when 'MECHANICAL' then 'MECHANICAL' "
            Sql &= "            WHEN 'PROCESS-STOKER' Then 'MECHANICAL' "
            Sql &= "            when 'OTHERS' then 'OTHERS' "
            Sql &= "            WHEN 'PROCESS' then 'PROCESS' "
            Sql &= "            WHEN 'STRUCTURE' THEN 'STRUCTURE' "
            Sql &= "            WHEN 'PIPING' THEN 'PIPING' "
            Sql &= "            WHEN 'C & I' THEN 'C&I' "
            Sql &= "            WHEN 'INSTRUMENTATION' THEN 'C&I' "
            Sql &= "            WHEN 'EPC' THEN 'EPC' "
            Sql &= "            WHEN 'C&I' THEN 'C&I' "
            Sql &= "            WHEN 'ELE' THEN 'ELECTRICAL' "
            Sql &= "            WHEN 'SERVICE' THEN 'OTHERS' "
            Sql &= "            else 'OTHERS' "
            Sql &= "            end) as Owner_Dept "
            Sql &= "            From tdmisg114200 as rec "
            Sql &= "LEFT JOIN ttppdm090200 "
            Sql &= "on ttppdm090200.t_cspa = rec.t_cspa "
            Sql &= "            Where t_cprj=('" & PrjID & "') and ttppdm090200.t_sort In ('MECHANICAL','MECH-SUGAR','PROCESS-STOKER')  and rec.t_stat=1 "

