            Sql &= "            Where rec.t_rcno Like ('REC%') and t_cprj=('" & PrjID & "')  and rec.t_stat=6"

          Case "Total_IDMSP_Superceded"
            Sql = "Select t_rcno, t_revn, t_cprj, t_item, Left(LTrim(t_item), 8) As element, "
            Sql &= "(case ttppdm090200.t_sort "
            Sql &= "            when 'MECH-SUGAR' then 'MECHANICAL' "
            Sql &= "            when 'ELECTRICAL' then 'ELECTRICAL' "
            Sql &= "            when 'MECHANICAL' then 'MECHANICAL' "
            Sql &= "            WHEN 'PROCESS-STOKER' Then 'MECHANICAL' "
            Sql &= "            when 'OTHERS' then 'OTHERS' "
            Sql &= "            WHEN 'PROCESS' then 'PROCESS' "
            Sql &= "            WHEN 'STRUCTURE' THEN 'STRUCTURE' "
            Sql &= "            WHEN 'PIPING' THEN 'PIPING' "
            Sql &= "            WHEN 'C & I' THEN 'C&I' "
            Sql &= "            WHEN 'INSTRUMENTATION' THEN 'C&I' "
            Sql &= "            WHEN 'EPC' THEN 'EPC' "
            Sql &= "            WHEN 'C&I' THEN 'C&I' "
            Sql &= "            WHEN 'ELE' THEN 'ELECTRICAL' "
            Sql &= "            WHEN 'SERVICE' THEN 'OTHERS' "
            Sql &= "            else 'OTHERS' "
            Sql &= "            end) as Owner_Dept,"
            Sql &= "            (case t_sent_1 "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IMechanical, "
            Sql &= "            (case t_sent_2  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IStructure, "
            Sql &= "            (case t_sent_3  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IPiping, "
            Sql &= "            (case t_sent_4  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IProcess, "
            Sql &= "            (case t_sent_5  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            when 2 then '-'   "
            Sql &= "            End ) As IC_I, "
            Sql &= "            (case t_sent_6  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            when 2 then '-'   "
            Sql &= "            End ) As IElectrical, "
            Sql &= "            (case t_sent_7 "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            when 2 then '-'   "
            Sql &= "            End ) As IQuality, (Case t_stat "
            Sql &= "            when 1 then 'Submitted' "
            Sql &= "            when 2 then 'Document linked' "
            Sql &= "            when 3 then 'Under Evaluation' "
            Sql &= "            when 4 then 'Comments Submitted' "
            Sql &= "            when 5 then 'Technically Cleared' "
            Sql &= "            when 6 then 'Transmittal Issued' "
            Sql &= "            when 7 then 'Superceded' "
            Sql &= "            when 8 then 'Closed' "
            Sql &= "            End) As t_stat "
            Sql &= "            From tdmisg134200 As rec "
            Sql &= " LEFT JOIN ttppdm090200 "
            Sql &= " on ttppdm090200.t_cspa= LEFT(Ltrim(rec.t_item),8) "
            Sql &= "            Where rec.t_rcno Like ('REC%') and t_cprj=('" & PrjID & "')  and rec.t_stat=7"
          Case "Total_IDMSP_Closed"
