            Sql &= "          when 2 then 'Under Review' "
            Sql &= "         When 3 Then 'Under Approval' "
            Sql &= "           when 4 then 'Approved' "
            Sql &= "          When 5 Then 'Not Applicable' "
            Sql &= "           End) As t_stat,rec.t_cspa as element, t_prep,t_rper,t_apby  "
            Sql &= "         From ttpisg074200 AS rec "
            Sql &= "LEFT JOIN ttppdm090200 "
            Sql &= "on ttppdm090200.t_cspa = rec.t_cspa "
            Sql &= "            Where t_cprj=('" & PrjID & "') and ttppdm090200.t_sort='STRUCTURE' AND rec.t_stat =1 "
            Sql &= "            Order by t_sarn"


