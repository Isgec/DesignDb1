            Sql &= "            (case t_sent_1 "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IMechanical, "
            Sql &= "            (case t_sent_2  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IStructure, "
            Sql &= "            (case t_sent_3  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IPiping, "
            Sql &= "            (case t_sent_4  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            When 2 Then '-'   "
            Sql &= "            End ) As IProcess, "
            Sql &= "            (case t_sent_5  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            when 2 then '-'   "
            Sql &= "            End ) As IC_I, "
            Sql &= "            (case t_sent_6  "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            when 2 then '-'   "
            Sql &= "            End ) As IElectrical, "
            Sql &= "            (case t_sent_7 "
            Sql &= "            when 1 then 'Response Pending' "
            Sql &= "            when 2 then '-'   "
            Sql &= "            End ) As IQuality, (Case t_stat "
            Sql &= "            when 1 then 'Submitted' "
            Sql &= "            when 2 then 'Document linked' "
            Sql &= "            when 3 then 'Under Evaluation' "
            Sql &= "            when 4 then 'Comments Submitted' "
            Sql &= "            when 5 then 'Technically Cleared' "
            Sql &= "            when 6 then 'Transmittal Issued' "
            Sql &= "            when 7 then 'Superceded' "
            Sql &= "            when 8 then 'Closed' "
            Sql &= "            End) As t_stat "
            Sql &= "            From tdmisg134200 As rec "
            Sql &= " LEFT JOIN ttppdm090200 "
            Sql &= " on ttppdm090200.t_cspa= LEFT(Ltrim(rec.t_item),8) "
            Sql &= "            Where rec.t_rcno Like ('REC%') and t_cprj=('" & PrjID & "') and ttppdm090200.t_sort='PROCESS' and rec.t_stat=2"
          Case "Process_IDMSP_Under_Evaluation"
            Sql = "Select t_rcno, t_revn, t_cprj, t_item, Left(LTrim(t_item), 8) As element, "
            Sql &= "(case ttppdm090200.t_sort "
            Sql &= "            when 'MECH-SUGAR' then 'MECHANICAL' "
            Sql &= "            when 'ELECTRICAL' then 'ELECTRICAL' "
